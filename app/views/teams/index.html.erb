<%= link_to "Conferences", "/conferences" %> | <%= link_to "Teams", "/teams" %>
<hr>

<% @teams.each do |team| %>
  <p>School Name: <%= team.school_name %></p>
  <p>Mascot: <%= team.mascot %></p>
  <p>Rank: <%= team.rank %></p>
  <p>Private School?: <%= team.private %></p>
  <p><%= button_to "Edit", "/teams/#{team.id}/edit", method: :get %></p>
<% end %>
